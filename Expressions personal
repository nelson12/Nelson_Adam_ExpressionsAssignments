//how big is a sand box?
var sandIn="sand";
var square="box";
var sandBox=sandIn+" "+square;
console.log(sandBox);//this is the concantenation part of the assignment;
var length=prompt("I am trying to calculate the volume of a sand box please enter length now");//declaring what length is and prompting for an answer;
var width= prompt("please enter width of sand box now");
var depth=prompt("please enter the depth of the sand box now");
var area=length*width*depth
alert ("the area of the sand box is"+" "+area);